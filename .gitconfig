[user]
  name = Ryan Bright
  email = ryan@rbright.net
[alias]
  cl = clone
  st = status
  co = checkout
  cm = commit
  pom = push origin master
  pod = push origin develop
  pl = pull
  ft = fetch
  rb = rebase
  amend = commit --amend
  unstage = reset HEAD --
  last = log -1 HEAD
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[color]
  ui = auto
[merge]
  tool = opendiff
[mergetool]
  keepBackup = true
[github]
  user = rbright
