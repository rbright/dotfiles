// Place your key bindings in this file to overwrite the defaults
[
  { "key": "cmd+ctrl+j", "command": "extension.dash.specific" },
  { "key": "ctrl+a", "command": "workbench.action.splitEditorRight" },
  { "key": "ctrl+d", "command": "workbench.action.splitEditorDown" },
  { "key": "ctrl+q", "command": "workbench.action.closeActiveEditor" },
  { "key": "ctrl+h", "command": "workbench.action.navigateLeft" },
  { "key": "ctrl+j", "command": "workbench.action.navigateDown" },
  { "key": "ctrl+k", "command": "workbench.action.navigateUp" },
  { "key": "ctrl+l", "command": "workbench.action.navigateRight" },
  { "key": "cmd+`", "command": "workbench.action.terminal.toggleTerminal" },
  { "key": "shift+cmd+d", "command": "workbench.view.debug" },
  { "key": "shift+cmd+m", "command": "workbench.actions.view.problems" },
  { "key": "shift+cmd+y", "command": "workbench.debug.action.toggleRepl" },
  {
    "key": "ctrl+w",
    "command": "extension.changeCase.upper",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+c",
    "command": "extension.changeCase.lower",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+e",
    "command": "extension.changeCase.camel",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+v",
    "command": "extension.changeCase.snake",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+j shift+cmd+m",
    "command": "cmstead.jsRefactor.extractMethod",
    "when": "editorTextFocus && vim.active && !inDebugRepl"
  }
]
