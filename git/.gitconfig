[color]
	ui = auto
[commit]
	gpgSign = true
[core]
	excludesfile = ~/.gitignore
[filter "lfs"]
	clean = git-lfs clean -- %f
	process = git-lfs filter-process
	required = true
	smudge = git-lfs smudge -- %f
[github]
	user = rbright
[gpg]
	program = /usr/local/bin/gpg
	format = openpgp
[gpg "ssh"]
	program = /Applications/1Password.app/Contents/MacOS/op-ssh-sign
	allowedSignersFile = 
[init]
	defaultBranch = main
[merge]
	tool = code
[mergetool]
	keepBackup = false
	prompt = false
[oh-my-zsh]
	git-commit-alias = 028d65363230b4aa54f3aaa14e03237243a2f1d8
[push]
	default = current
[tag]
	forceSignAnnotated = false
