{
  "agent": {
    "agents_panel_dock": "left",
    "button": true,
    "default_width": 360,
    "dock": "left",
    "enable_feedback": true,
    "enabled": true,
    "inline_assistant_model": {
      "model": "gpt-5.2",
      "provider": "zed.dev"
    },
    "model_parameters": [],
    "play_sound_when_agent_done": true
  },
  "audio": {
    "experimental.legacy_audio_compatible": false
  },
  "auto_update": true,
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "base_keymap": "Cursor",
  "buffer_font_family": "JetBrainsMono Nerd Font Mono",
  "buffer_font_features": {
    "calt": false,
    "liga": false
  },
  "buffer_font_size": 15,
  "buffer_font_weight": 700,
  "buffer_line_height": "standard",
  "calls": {
    "mute_on_join": true,
    "share_on_join": false
  },
  "collaboration_panel": {
    "button": false,
    "default_width": 240,
    "dock": "left"
  },
  "confirm_quit": false,
  "cursor_shape": "bar",
  "diagnostics": {
    "button": true,
    "include_warnings": true,
    "inline": {
      "enabled": true,
      "max_severity": null,
      "update_debounce_ms": 150
    },
    "lsp_pull_diagnostics": {
      "debounce_ms": 50,
      "enabled": true
    }
  },
  "enable_language_server": true,
  "ensure_final_newline_on_save": true,
  "file_scan_exclusions": [
    "**/__pycache__",
    "**/.bun-cache",
    "**/.bun-tmp",
    "**/.coverage",
    "**/.direnv",
    "**/.DS_Store",
    "**/.git",
    "**/.hg",
    "**/.husky/_",
    "**/.jj",
    "**/.pytest_cache",
    "**/.ruff_cache",
    "**/.storybook",
    "**/.svn",
    "**/.venv",
    "**/build",
    "**/cassettes",
    "**/coverage",
    "**/dist",
    "**/node_modules",
    "**/reports",
    "**/.map.js"
  ],
  "file_types": {
    "Dockerfile": [
      "Dockerfile*"
    ],
    "Helm": [
      "**/helmfile.d/**/*.yaml",
      "**/helmfile.d/**/*.yml",
      "**/templates/**/*.tpl",
      "**/templates/**/*.yaml",
      "**/templates/**/*.yml"
    ],
    "JSONC": [
      "**/.vscode/**/*.json",
      "**/.zed/**/*.json",
    ],
    "Shell Script": [
      "**/.curlrc",
      "**/.env.*",
      "**/.inputrc",
      "**/skhdrc",
      "**/tmux.conf",
      "**/*.tmux",
    ]
  },
  "format_on_save": "off",
  "git": {
    "git_gutter": "tracked_files",
    "inline_blame": {
      "delay_ms": 300,
      "enabled": true,
      "show_commit_summary": true
    }
  },
  "git_panel": {
    "button": true,
    "collapse_untracked_diff": false,
    "default_width": 240,
    "dock": "right",
    "scrollbar": {
      "show": "never"
    },
    "sort_by_path": false,
    "status_style": "icon",
    "tree_view": true
  },
  "hard_tabs": false,
  "hover_popover_delay": 300,
  "hover_popover_enabled": true,
  "icon_theme": "Catppuccin Mocha",
  "indent_guides": {
    "active_line_width": 1,
    "background_coloring": "disabled",
    "coloring": "indent_aware",
    "enabled": true,
    "line_width": 1
  },
  "inlay_hints": {
    "edit_debounce_ms": 700,
    "enabled": true,
    "scroll_debounce_ms": 50,
    "show_background": true,
    "show_other_hints": true,
    "show_parameter_hints": true,
    "show_type_hints": true,
    "show_value_hints": true
  },
  "journal": {
    "hour_format": "hour24"
  },
  "languages": {
    "Go": {
      "format_on_save": "on",
      "formatter": "language_server",
      "preferred_line_length": 120,
      "tab_size": 2
    },
    "GraphQL": {
      "format_on_save": "on",
      "preferred_line_length": 80,
      "tab_size": 2
    },
    "HCL": {
      "format_on_save": "on",
      "preferred_line_length": 80,
      "tab_size": 2
    },
    "JSON": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      },
      "format_on_save": "on",
      "formatter": "language_server",
      "preferred_line_length": 80,
      "prettier": {
        "allowed": true
      },
      "tab_size": 2
    },
    "JSONC": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      },
      "format_on_save": "on",
      "formatter": "language_server",
      "preferred_line_length": 80,
      "prettier": {
        "allowed": false
      },
      "tab_size": 2
    },
    "JavaScript": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      },
      "format_on_save": "on",
      "formatter": [],
      "preferred_line_length": 120,
      "tab_size": 2
    },
    "Just": {
      "format_on_save": "on",
      "formatter": {
        "external": {
          "arguments": [
            "-lc",
            "set -euo pipefail; tmp=\"$(mktemp)\"; cat >\"$tmp\"; just --justfile \"$tmp\" --dump; rm -f \"$tmp\""
          ],
          "command": "bash"
        }
      },
      "preferred_line_length": 80,
      "tab_size": 2
    },
    "Markdown": {
      "format_on_save": "on",
      "formatter": "prettier",
      "remove_trailing_whitespace_on_save": true,
      "tab_size": 2
    },
    "Nix": {
      "format_on_save": "on",
      "preferred_line_length": 80,
      "tab_size": 2
    },
    "Python": {
      "auto_indent": true,
      "colorize_brackets": true,
      "format_on_save": "on",
      "formatter": "language_server",
      "indent_guides": {
        "enabled": true
      },
      "language_servers": [
        "ruff",
        "pyrefly",
        "!pyright",
        "!pylsp"
      ],
      "preferred_line_length": 120,
      "tab_size": 4,
      "use_autoclose": true
    },
    "Rust": {
      "format_on_save": "on",
      "formatter": "language_server",
      "preferred_line_length": 120,
      "tab_size": 4
    },
    "SQL": {
      "format_on_save": "on",
      "formatter": {
        "external": {
          "command": "uv run sqlfluff format"
        }
      },
      "preferred_line_length": 80,
      "tab_size": 4
    },
    "Shell Script": {
      "formatter": {
        "external": {
          "arguments": [
            "--filename",
            "{buffer_path}",
            "--indent",
            "2"
          ],
          "command": "shfmt"
        }
      },
      "hard_tabs": false,
      "preferred_line_length": 80,
      "tab_size": 2
    },
    "TOML": {
      "format_on_save": "on",
      "formatter": "language_server",
      "tab_size": 2
    },
    "TSX": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      },
      "format_on_save": "on",
      "formatter": "language_server",
      "tab_size": 2
    },
    "Terraform": {
      "format_on_save": "on",
      "formatter": "language_server",
      "preferred_line_length": 80,
      "tab_size": 2
    },
    "Terraform Vars": {
      "format_on_save": "on",
      "formatter": "language_server",
      "preferred_line_length": 80,
      "tab_size": 2
    },
    "TypeScript": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      },
      "format_on_save": "on",
      "formatter": [],
      "prettier": {
        "allowed": false
      },
      "tab_size": 2
    },
    "XML": {
      "format_on_save": "on",
      "formatter": "language_server",
      "tab_size": 2
    },
    "YAML": {
      "format_on_save": "on",
      "formatter": "prettier",
      "tab_size": 2
    }
  },
  "lsp": {
    "json-language-server": {
      "initialization_options": {
        "provideFormatter": true
      },
      "settings": {
        "json": {
          "format": {
            "enable": true
          },
          "validate": {
            "enable": true
          }
        }
      }
    },
    "pyrefly": {
      "binary": {
        "arguments": [
          "lsp"
        ],
        "path": ".venv/bin/pyrefly"
      }
    },
    "rust-analyzer": {
      "initialization_options": {
        "inlayHints": {
          "closureReturnTypeHints": {
            "enable": "always"
          },
          "lifetimeElisionHints": {
            "enable": "skip_trivial",
            "useParameterNames": true
          },
          "maxLength": null
        }
      }
    },
    "tailwindcss-language-server": {
      "settings": {
        "experimental": {
          "classRegex": [
            "\\.className\\s*[+]?=\\s*['\"]([^'\"]*)['\"]",
            "\\.setAttributeNS\\(.*,\\s*['\"]class['\"],\\s*['\"]([^'\"]*)['\"]",
            "\\.setAttribute\\(['\"]class['\"],\\s*['\"]([^'\"]*)['\"]",
            "\\.classList\\.add\\(['\"]([^'\"]*)['\"]",
            "\\.classList\\.remove\\(['\"]([^'\"]*)['\"]",
            "\\.classList\\.toggle\\(['\"]([^'\"]*)['\"]",
            "\\.classList\\.contains\\(['\"]([^'\"]*)['\"]",
            "\\.classList\\.replace\\(\\s*['\"]([^'\"]*)['\"]",
            "\\.classList\\.replace\\([^,)]+,\\s*['\"]([^'\"]*)['\"]"
          ]
        }
      }
    },
    "terraform-ls": {
      "initialization_options": {
        "experimentalFeatures": {
          "prefillRequiredFields": true
        }
      }
    },
    "typescript-language-server": {
      "initialization_options": {
        "preferences": {
          "includeInlayEnumMemberValueHints": true,
          "includeInlayFunctionLikeReturnTypeHints": true,
          "includeInlayFunctionParameterTypeHints": true,
          "includeInlayParameterNameHints": "all",
          "includeInlayParameterNameHintsWhenArgumentMatchesName": true,
          "includeInlayPropertyDeclarationTypeHints": true,
          "includeInlayVariableTypeHints": true,
          "includeInlayVariableTypeHintsWhenTypeMatchesName": true
        }
      }
    },
    "vtsls": {
      "settings": {
        "javascript": {
          "inlayHints": {
            "enumMemberValues": {
              "enabled": true
            },
            "functionLikeReturnTypes": {
              "enabled": true
            },
            "parameterNames": {
              "enabled": "all",
              "suppressWhenArgumentMatchesName": false
            },
            "parameterTypes": {
              "enabled": true
            },
            "propertyDeclarationTypes": {
              "enabled": true
            },
            "variableTypes": {
              "enabled": true,
              "suppressWhenTypeMatchesName": true
            }
          },
          "tsserver": {
            "maxTsServerMemory": 16184
          }
        },
        "typescript": {
          "inlayHints": {
            "enumMemberValues": {
              "enabled": true
            },
            "functionLikeReturnTypes": {
              "enabled": true
            },
            "parameterNames": {
              "enabled": "all",
              "suppressWhenArgumentMatchesName": false
            },
            "parameterTypes": {
              "enabled": true
            },
            "propertyDeclarationTypes": {
              "enabled": true
            },
            "variableTypes": {
              "enabled": true,
              "suppressWhenTypeMatchesName": true
            }
          },
          "tsserver": {
            "maxTsServerMemory": 16184
          }
        }
      }
    },
    "yaml-language-server": {
      "settings": {
        "yaml": {
          "format": {
            "singleQuote": true
          },
          "validate": true
        }
      }
    }
  },
  "notification_panel": {
    "button": true,
    "default_width": 240,
    "dock": "right"
  },
  "outline_panel": {
    "auto_fold_dirs": true,
    "auto_reveal_entries": true,
    "button": true,
    "default_width": 240,
    "dock": "right",
    "expand_outlines_with_depth": null,
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "indent_guides": {
      "show": "always"
    },
    "indent_size": 20,
    "scrollbar": {
      "show": "never"
    }
  },
  "project_panel": {
    "auto_fold_dirs": true,
    "auto_reveal_entries": true,
    "button": true,
    "default_width": 240,
    "dock": "right",
    "entry_spacing": "standard",
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "indent_guides": {
      "show": "always"
    },
    "indent_size": 20,
    "scrollbar": {
      "show": "never"
    },
    "show_diagnostics": "all",
    "sort_mode": "directories_first",
    "starts_open": true
  },
  "relative_line_numbers": "enabled",
  "remove_trailing_whitespace_on_save": true,
  "restore_on_startup": "last_session",
  "scrollbar": {
    "diagnostics": "all",
    "git_diff": false,
    "search_results": true
  },
  "show_call_status_icon": true,
  "show_completion_documentation": true,
  "show_completions_on_input": true,
  "show_whitespaces": "all",
  "soft_wrap": "none",
  "tab_bar": {
    "show": true,
    "show_nav_history_buttons": false,
    "show_tab_bar_buttons": false
  },
  "tabs": {
    "activate_on_close": "history",
    "close_position": "right",
    "file_icons": true,
    "git_status": true,
    "show_close_button": "hover",
    "show_diagnostics": "all"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "alternate_scroll": "off",
    "blinking": "terminal_controlled",
    "button": false,
    "copy_on_select": false,
    "cursor_shape": "block",
    "detect_venv": {
      "on": {
        "activate_script": "default",
        "directories": [
          ".env",
          "env",
          ".venv",
          "venv"
        ]
      }
    },
    "dock": "bottom",
    "env": {
      "EDITOR": "zed --wait"
    },
    "font_family": "JetBrainsMono Nerd Font Mono",
    "font_features": {
      "calt": false,
      "liga": false
    },
    "font_size": 15,
    "font_weight": 700,
    "line_height": "standard",
    "option_as_meta": false,
    "scrollbar": {
      "show": null
    },
    "shell": {
      "program": "nu"
    },
    "toolbar": {
      "breadcrumbs": true
    },
    "working_directory": "current_project_directory"
  },
  "theme": "Catppuccin Mocha",
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false
  },
  "ui_font_family": "JetBrainsMono Nerd Font Mono",
  "ui_font_features": {
    "calt": false,
    "liga": false
  },
  "ui_font_size": 15,
  "ui_font_weight": 700,
  "unnecessary_code_fade": 0.5,
  "use_autoclose": true,
  "vim_mode": true
}
